{
  "extends": "astro/tsconfigs/strictest",
  "exclude": ["packages"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@images/*": ["src/images/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@contracts/*": ["src/contracts/*"],
      "@lib/*": ["src/lib/*"]
    }
  }
}
